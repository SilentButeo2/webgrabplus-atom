{
  "name": "webgrabplus-atom",
  "main": "./lib/webgrabplus-atom",
  "version": "0.1.3",
  "description": "WebGrab+Plus support package",
  "keywords": [],
  "activationCommands": {},
  "repository": "https://github.com/SilentButeo2/webgrabplus-atom.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
