      "description": "Runs WebGrab+Plus config files (siteini's)",
      "versions": {
        "2.0.0": "buildprovider"
      }
    }
  },
  "package-deps": [
    "build"
  ],
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  },
  "configSchema": {
    "path-wg": {
